==>
    Test program
<==

fun test(arg0, arg1) [
    var dll = dllopen "/usr/lib64/libc.so.6"
    var write = getsym dll "write"

    if arg0 == arg1
    [
        ccall write 1 "ok\n" 4
        return arg0 - arg1
    ]
    else
    [
        ccall write 1 "not_ok" 6
        return arg0 + arg1
    ]
]

test(2, 2)
